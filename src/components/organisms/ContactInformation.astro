---
import CardContact from "../molecules/CardContact.astro";
import contacts from "../../data/contact.json";

console.log(contacts);
---

<section class="bg-background-primary p-10 rounded-full @container">
  <header class="py-8 grid gap-4 max-w-[420px]">
    <h2 class="text-heading-medium text-pretty">Her står der et fedt ord!</h2>
    <p class="text-pretty">Du behøver ikke at have styr på alle detaljerne, dem finder vi ud af sammen</p>
  </header>

  <ul class="grid @2xl:grid-cols-3 gap-11">
    {
      contacts.map((contact) => {
        return <CardContact data={contact} />;
      })
    }
  </ul>
</section>
